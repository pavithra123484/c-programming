#include <stdio.h>

int main() {
    int a, b, num1, num2, gcd, lcm;

    // Read two numbers
    scanf("%d %d", &a, &b);

    num1 = a;
    num2 = b;

    // Find GCD using Euclidean algorithm
    while (b != 0) {
        int temp = b;
        b = a % b;
        a = temp;
    }
    gcd = a;

    // Calculate LCM using the formula
    lcm = (num1 * num2) / gcd;

    // Print results
    printf("GCD = %d\n", gcd);
    printf("LCM = %d", lcm);

    return 0;
}