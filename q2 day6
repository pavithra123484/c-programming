#include <stdio.h>

int main() {
    int a, b, c;
    scanf("%d %d %d", &a, &b, &c);

    // Finding greatest
    if (a >= b && a >= c)
        printf("Max: %d\n", a);
    else if (b >= a && b >= c)
        printf("Max: %d\n", b);
    else
        printf("Max: %d\n", c);

    // Finding smallest
    if (a <= b && a <= c)
        printf("Min: %d", a);
    else if (b <= a && b <= c)
        printf("Min: %d", b);
    else
        printf("Min: %d", c);

    return 0;
}