#include <stdio.h>
#include <math.h>

int main() {
    int n, original, remainder, result = 0, digits = 0;

    // Read input number
    scanf("%d", &n);
    original = n;

    // Count number of digits
    int temp = n;
    while (temp != 0) {
        temp /= 10;
        digits++;
    }

    // Calculate sum of powers of digits
    temp = n;
    while (temp != 0) {
        remainder = temp % 10;
        result += pow(remainder, digits);
        temp /= 10;
    }

    // Check Armstrong condition
    if (result == original)
        printf("Armstrong number");
    else
        printf("Not Armstrong number");

    return 0;
}